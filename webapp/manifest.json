{
  "_version": "1.65.0",
  "sap.app": {
    "id": "employee.directory",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.18.4",
      "toolsId": "2463201b-0cbc-44a3-a888-9457e5753512"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.65.6",
      "libs": {
        "sap.ui.layout": {},
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "model/employees.json"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "employee.directory.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "employee.directory.view",
        "async": true
      },
      "routes": [
        {
          "name": "employeeList",
          "pattern": "",
          "target": ["TargetEmployeeList"]
        },
        {
          "pattern": "employee/{employeeId}",
          "name": "employeeDetails",
          "target": ["TargetEmployeeDetails"]
        }
      ],
      "targets": {
        "TargetEmployeeList": {
          "viewId": "EmployeeList",
          "viewName": "EmployeeList",
          "viewType": "XML"
        },
        "TargetEmployeeDetails": {
          "viewId": "EmployeeDetails",
          "viewName": "EmployeeDetails",
          "viewType": "XML",
          "viewLevel": 1
        }
      }
    },
    "rootView": {
      "viewName": "employee.directory.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
